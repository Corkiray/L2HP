(define
   (problem bloques_problem)
   (:domain bloques)

   (:objects 
      blue - block
      yellow - block
      red - block
      green - block
      grip - gripper
   )

   (:init
      (on blue red)
      (on red yellow)
      (on_table yellow)
      (on_table green)
      (clear blue)
      (clear green)
      (empty grip)
   )

   (:goal
      (and 
         (on red green) 
      )
   )

)